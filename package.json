{
    "name": "sign-in-boilerplate",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@githubprimer/octicons-react": "^8.5.0",
        "bootstrap": "^4.3.1",
        "firebase": "^5.9.0",
        "firebase-admin": "^7.1.0",
        "formik": "^1.5.1",
        "node-sass-chokidar": "^1.3.4",
        "npm-run-all": "^4.1.5",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-dropzone": "^10.0.6",
        "react-router-dom": "^4.4.0",
        "react-scripts": "2.1.8",
        "react-tooltip": "^3.10.0",
        "react-use-form-state": "^0.9.0",
        "recompose": "^0.30.0"
    },
    "scripts": {
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules src/ -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-script build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
