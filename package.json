{
    "name": "sign-in-boilerplate",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@githubprimer/octicons-react": "^8.5.0",
        "@react-firebase/database": "^0.3.10",
        "bootstrap": "^4.3.1",
        "firebase": "^5.9.0",
        "firebase-admin": "^7.3.0",
        "firebase-functions": "^2.3.1",
        "formik": "^1.5.1",
        "jquery": "^3.4.1",
        "moment": "^2.24.0",
        "node-sass-chokidar": "^1.3.4",
        "npm-run-all": "^4.1.5",
        "nuka-carousel": "^4.5.8",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-dropzone": "^10.0.6",
        "react-infinite-scroller": "^1.2.4",
        "react-rater": "^5.0.5",
        "react-responsive-carousel": "^3.1.49",
        "react-router-dom": "^4.4.0",
        "react-scripts": "2.1.8",
        "react-select": "^2.4.2",
        "react-tooltip": "^3.10.0",
        "react-transition-group": "^2.8.0",
        "react-use-form-state": "^0.9.0",
        "recompose": "^0.30.0",
        "shallowequal": "^1.1.0"
    },
    "scripts": {
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules src/ -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-script build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "react-alice-carousel": "^1.13.13"
    }
}
